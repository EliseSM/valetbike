<div class="record-row <%= cycle "odd", "even", name: "row-cycler" %> flexbox vertical stretch">
  <%= station.identifier %>: <%= station.name %> (<%= station.address %>) // total bike count: <%= station.docked_bikes.count %>
</div>

<% station.docked_bikes.each do |bike| %>
  <div class="record-row <%= cycle "odd", "even", name: "row-cycler" %> flexbox vertical stretch">
    ======= bike: <%= bike.identifier%> station: <%= bike.current_station.name%>
  </div>
  <% end %>

  

